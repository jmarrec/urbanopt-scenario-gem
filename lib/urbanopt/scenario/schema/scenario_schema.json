{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Comment describing your JSON Schema",
	"type": "object",
	"properties": {
		"scenario_id": {
			"type": "string"
		},
		"scenario_name": {
			"type": "string"
		},
		"scenario_directory": {
			"type": "string"
		},
		"scenario_results": {
			"description": "Aggregated results over the entire scenario",
			"type": "object",
			"properties": {
				"location": {
					"$ref": "#/definitions/Location"
				},
				"timesteps_per_hour": {
					"$ref": "#/definitions/TimestepsPerHour"
				},
				"reporting_periods": {
					"$ref": "#/definitions/SimulationPeriods"
				},
				"scenario_program": {
					"$ref": "#/definitions/Program"
				}
			}
		},
		"features": {
			"description": "features of each data point in the scenario",
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"feature_id": {
						"type": "string"
					},
					"feature_name": {
						"type": "string"
					},
					"feature_directory": {
						"type": "string"
					},
					"simulatioin_status": {
						"type": "string",
						"enum": [
							"Not Started",
							"Started",
							"Complete",
							"Failed"
						]
					},
					"feature_results": {
						"type": "object",
						"properties": {
							"location": {
								"$ref": "#/definitions/Location"
							},
							"timesteps_per_hour": {
								"$ref": "#/definitions/TimestepsPerHour"
							},
							"reporting_periods": {
								"$ref": "#/definitions/SimulationPeriods"
							},
							"feature_program": {
								"$ref": "#/definitions/Program"
							}
						}
					}
				}
			}
		}
	},
	"definitions": {
		"SimulationPeriod": {
			"type": "object",
			"properties": {
				"simulation_period_id": {
					"type": "string"
				},
				"simulation_period_name": {
					"type": "string"
				},
				"start_date": {
					"type": "object",
					"properties": {
						"begin_month": {
							"type": "integer"
						},
						"begin_day_of_month": {
							"type": "integer"
						},
						"begin_year": {
							"type": "integer"
						}
					}
				},
				"end_date": {
					"type": "object",
					"properties": {
						"end_month": {},
						"end_day_of_month": {}
					}
				},
				"energy_result": {
					"$ref": "#/definitions/EnergyResults"
				},
				"cost_results": {
					"$ref": "#/definitions/CostResults"
				}
			}
		},
		"SimulationPeriods": {
			"type": "object",
			"properties": {
				"reporting_period": {
					"$ref": "#/definitions/SimulationPeriod"
				}
			}
		},
		"DetailedConstructionCost": {
			"description": "Program related information, does not change in time",
			"type": "object",
			"properties": {
				"total_cost": {
					"description": "($)",
					"type": "number"
				},
				"cost_type": {
					"type": "string",
					"enum": [
						"Capital Cost"
					]
				}
			}
		},
		"DetailedEnergyCost": {
			"description": "Program related information, does not change in time",
			"type": "object",
			"properties": {
				"fuel_type": {
					"type": "string",
					"enum": [
						"Electricity",
						"Natural Gas"
					]
				},
				"total_cost": {
					"description": "($)",
					"type": "number"
				},
				"usage_cost": {
					"description": "($)",
					"type": "number"
				},
				"demand_cost": {
					"description": "($)",
					"type": "number"
				}
			}
		},
		"CostResults": {
			"description": "Program related information, does not change in time",
			"type": "object",
			"properties": {
				"life_cycle_period": {
					"description": "Years",
					"type": "number"
				},
				"life_cycle_cost": {
					"description": "$",
					"type": "number"
				},
				"energy_costs": {
					"description": "($)",
					"type": "array",
					"items": {
						"$ref": "#/definitions/DetailedEnergyCost"
					}
				},
				"construction_costs": {
					"description": "($)",
					"type": "array",
					"items": {
						"$ref": "#/definitions/DetailedConstructionCost"
					}
				}
			}
		},
		"Location": {
			"type": "object",
			"properties": {
				"latitude": {
					"description": "(deg)",
					"type": "number"
				},
				"longitude": {
					"description": "(deg)",
					"type": "number"
				},
				"elevation": {
					"description": "(ft)",
					"type": "number"
				},
				"weather_file": {
					"description": "(deg)",
					"type": "string"
				}
			}
		},
		"TimestepsPerHour": {
			"type": "integer",
			"minimum": 1,
			"maximum": 60
		},
		"Program": {
			"description": "Program related information, does not change in time",
			"type": "object",
			"properties": {
				"total_site_area": {
					"description": "Site gross area (ft^2)",
					"type": "number"
				},
				"total_building_floor_area": {
					"description": "Building gross floor area (ft^2)",
					"type": "number"
				},
				"total_building_footprint_area": {
					"description": "Building gross floorprint area (ft^2)",
					"type": "number"
				},
				"total_parking_floor_area": {
					"description": "Parking gross floorprint area (ft^2)",
					"type": "number"
				},
				"total_parking_footprint_area": {
					"description": "Parking gross floorprint area (ft^2)",
					"type": "number"
				},
				"window_area": {
					"description": "Exterior windows gross area (ft^2)",
					"type": "object",
					"properties": {
						"north_window_area": {
							"type": "number"
						},
						"south_window_area": {
							"type": "number"
						},
						"east_window_area": {
							"type": "number"
						},
						"west_window_area": {
							"type": "number"
						},
						"total_window_area": {
							"type": "number"
						}
					}
				},
				"wall_area": {
					"description": "exterior walls gross area (ft^2)",
					"type": "object",
					"properties": {
						"north_wall_area": {
							"type": "number"
						},
						"south_wall_area": {
							"type": "number"
						},
						"east_wall_area": {
							"type": "number"
						},
						"west_wall_area": {
							"type": "number"
						},
						"total_wall_area": {
							"type": "number"
						}
					}
				},
				"total_roof_area": {
					"description": "Exterior roofs gross area (ft^2)",
					"type": "number"
				},
				"maximum_height": {
					"type": "number"
				},
				"available_roof_area": {
					"description": "gross roof area (ft^2)",
					"type": "number"
				},
				"building_types": {
					"description": "A list of building types",
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"building_type": {
								"description": "Building program (Commercial,Residential, Multifamily etc.)",
								"type": "string"
							},
							"floor_area": {
								"description": "building gross floor area",
								"type": "number"
							}
						}
					}
				},
				"occupancy": {
					"type": "object",
					"properties": {
						"total_occupancy": {
							"description": "sum of all occupant in a building",
							"type": "integer"
						},
						"occupant_density": {
							"description": "(people/ft^2)",
							"type": "number"
						}
					}
				},
				"conditioned_area": {
					"description": "(ft^2)",
					"type": "number"
				},
				"unconditioned_area": {
					"description": "(ft^2)",
					"type": "number"
				},
				"orientation": {
					"description": "feature results only",
					"type": "number"
				},
				"aspect_ratio": {
					"description": "feature results only",
					"type": "number"
				}
			}
		},
		"EndUse": {
			"type": "object",
			"properties": {
				"heating": {
					"type": "number"
				},
				"cooling": {
					"type": "number"
				},
				"interior_lighting": {
					"type": "number"
				},
				"exterior_lighting": {
					"type": "number"
				},
				"interior_equipment": {
					"type": "number"
				},
				"exterior_equipment": {
					"type": "number"
				},
				"fans": {
					"type": "number"
				},
				"pumps": {
					"type": "number"
				},
				"heat_rejection": {
					"type": "number"
				},
				"humidification": {
					"type": "number"
				},
				"heat_recovery": {
					"type": "number"
				},
				"water_systems": {
					"type": "number"
				},
				"refrigeration": {
					"type": "number"
				},
				"generators": {
					"type": "number"
				}
			}
		},
		"EnergyResults": {
			"type": "object",
			"properties": {
				"end_use_eui": {
					"description": "(kBtu/ft^2)",
					"type": "object",
					"properties": {
						"electricity_energy": {
							"$ref": "#/definitions/EndUse"
						},
						"natural_gas_energy": {
							"$ref": "#/definitions/EndUse"
						},
						"additional_fuel_energy": {
							"$ref": "#/definitions/EndUse"
						},
						"district_energy": {
							"$ref": "#/definitions/EndUse"
						},
						"water_energy": {
							"$ref": "#/definitions/EndUse"
						}
					}
				},
				"end_use_energy": {
					"description": "(kBtu)",
					"type": "object",
					"properties": {
						"electricity_energy": {
							"$ref": "#/definitions/EndUse"
						},
						"natural_gas_energy": {
							"$ref": "#/definitions/EndUse"
						},
						"additional_fuel_energy": {
							"$ref": "#/definitions/EndUse"
						},
						"district_energy": {
							"$ref": "#/definitions/EndUse"
						},
						"water_energy": {
							"$ref": "#/definitions/EndUse"
						}
					}
				},
				"total_end_use_energy": {
					"type": "object",
					"properties": {
						"total_electricity": {
							"description": "Sum of all electricity end uses (kBtu)",
							"type": "number"
						},
						"total_natural_gas": {
							"description": "Sum of all natural gas end uses consumption (kBtu)",
							"type": "number"
						},
						"total_additional_fuel": {
							"description": "Sum of all additional fuel end uses consumption (kBtu)",
							"type": "number"
						},
						"total_district_cooling": {
							"description": "Sum of all distric cooling end uses consumption (kBtu)",
							"type": "number"
						},
						"total_district_heating": {
							"description": "Sum of all distric heating end uses consumption (kBtu)",
							"type": "number"
						},
						"total_water": {
							"description": "Sum of all water end uses consumption (ft^3)",
							"type": "number"
						},
						"total_site_energy": {
							"description": "(kBtu)",
							"type": "number"
						},
						"total_site_energy_eui": {
							"description": "(kBtu/ft^2)",
							"type": "number"
						},
						"total_source_energy": {
							"description": "(kBtu)",
							"type": "number"
						},
						"total_source_energy_eui": {
							"description": "(kBtu/ft^2)",
							"type": "number"
						}
					}
				},
				"Facility": {
					"type": "object",
					"properties": {
						"electricity": {
							"type": "number"
						},
						"electricity_produced": {
							"type": "number"
						},
						"gas": {
							"type": "number"
						},
						"district_cooling": {
							"type": "number"
						},
						"district_heating": {
							"type": "number"
						},
						"district_cooling_chilled_water_rate": {
							"type": "number"
						},
						"district_cooling_mass_flow_rate": {
							"type": "number"
						},
						"district_cooling_inlet_temperature": {
							"type": "number"
						},
						"district_cooling_outlet_temperature": {
							"type": "number"
						},
						"district_heating_hot_water_rate": {
							"type": "number"
						},
						"district_heating_mass_flow_rate": {
							"type": "number"
						},
						"district_heating_inlet_temperature": {
							"type": "number"
						},
						"district_heating_outlet_temperature": {
							"type": "number"
						}
					}
				},
				"time_setpoint_not_met": {
					"description": "(hr)",
					"type": "object",
					"properties": {
						"time_setpoint_not_met_during_occupied_heating": {
							"type": "number"
						},
						"time_setpoint_not_met_during_occupied_cooling": {
							"type": "number"
						},
						"time_setpoint_not_met_during_occupied_hours": {
							"type": "number"
						}
					}
				}
			}
		}
	}
}